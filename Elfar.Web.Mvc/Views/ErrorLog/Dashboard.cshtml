<div id="dashboard" data-bind="foreach: sections">
    <div class="section" data-bind="attr: { id: name }">
        <div class="head" data-bind="text: title"></div>
        <div class="body" data-bind="template: template"></div>
    </div>
</div>
<script type="text/html" id="summary-template">
    <ul data-bind="foreach: tiles">
        <li data-bind="template: { name: template, data: content }, css: size"></li>
    </ul>
</script>
<script type="text/html" id="term-tile-template">
    <div data-bind="click: count ? $root.add : null, css: css">
        <div class="head" data-bind="attr: { title: title }, text: title"></div>
        <div class="body" data-bind="text: count"></div>
        <div class="foot">Error Logs</div>
    </div>
</script>
<script type="text/html" id="donut-template">
    <div class="chart" data-bind="attr: { id: id }"></div>
    <div class="title" data-bind="text: $data"></div>
    <ul class="legend" data-bind="foreach: series">
        <li data-bind="attr: { title: key }">
            <span data-bind="style: { backgroundColor: colour }"></span>
            <span data-bind="text: key.controller"></span>
        </li>
    </ul>
</script>
<script type="text/html" id="chart-template">
    <div class="chart" data-bind="attr: { id: id }"></div>
</script>
<script type="text/html" id="latest-template">
    <table id="latest">
        <colgroup>
           <col class="type" />
        </colgroup>
        <thead>
            <tr>
                <th class="type">Type</th>
                <th class="small centre">Date</th>
                <th class="small centre">Time</th>
            </tr>
        </thead>
        <tbody data-bind="foreach: summaries">
            <tr>
                <td data-bind="attr: { title: Type }"><div><a data-bind="text: Type"></a></div></td>
                <td data-bind="text: Date"></td>
                <td data-bind="text: Time"></td>
            </tr>
        </tbody>
    </table>
</script>
<script type="text/html" id="common-template">
    <table id="common">
        <thead>
            <tr>
                <th class="type">Type</th>
                <th class="small centre">Count</th>
            </tr>
        </thead>
        <tbody data-bind="foreach: summaries">
            <tr>
                <td data-bind="attr: { title: key }"><a data-bind="text: key"></a></td>
                <td data-bind="text: length"></td>
            </tr>
        </tbody>
    </table>
</script>

