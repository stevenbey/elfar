<div id="dashboard" data-bind="foreach: sections">
    <div class="section" data-bind="attr: { id: name }">
        <div class="section">
            <div class="section-title" data-bind="text: title"></div>
            <div class="section-content" data-bind="template: template"></div>
        </div>
    </div>
</div>
<script type="text/html" id="statistics-template">
    <ul data-bind="foreach: items">
        <li data-bind="template: { name: template, data: content }, css: size"></li>
    </ul>
</script>
<script type="text/html" id="term-tile-template">
    <div data-bind="click: count ? $root.addTab : null, css: css"><span data-bind="text: title"></span> <span class="count" data-bind="text: count"></span></div>
</script>
<script type="text/html" id="donut-template">
    <div id="donut"></div>
</script>

