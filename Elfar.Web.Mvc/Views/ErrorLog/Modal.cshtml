<div class="modal" id="errorLog" tabindex="-1" role="dialog" aria-labelledby="errorLog-title" aria-hidden="true" data-bind="with: errorLog">
    <div>
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title" id="errorLog-title">
                        <span data-bind="text: Type + ':'"></span>
                        <span data-bind="text: Url"></span>
                    </h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <label>Message</label><input readonly="readonly" data-bind="value: Message" />
                    </div>
                    <div class="row">
                        <div class="col-xs-6">
                            <label>Type</label><input readonly="readonly" data-bind="value: Type" />
                        </div>
                        <div class="col-xs-6">
                            <label>Date</label><input readonly="readonly" data-bind="value: Date" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-6">
                            <label>Source</label><input readonly="readonly" data-bind="value: Source" />
                        </div>
                        <div class="col-xs-6">
                            <label>Time</label><input readonly="readonly" data-bind="value: Time" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-6">
                            <label>Url</label><input readonly="readonly" data-bind="value: Url" />
                        </div>
                        <div class="col-xs-6">
                            <label>Host</label><input readonly="readonly" data-bind="value: Host" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-6">
                            <label>Action</label><input readonly="readonly" data-bind="value: Action" />
                        </div>
                        <div class="col-xs-6">
                            <label>Area</label><input readonly="readonly" data-bind="value: Area" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-6">
                            <label>Controller</label><input readonly="readonly" data-bind="value: Controller" />
                        </div>
                        <div class="col-xs-6">
                            <label>Method</label><input readonly="readonly" data-bind="value: HttpMethod" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-6">
                            <label>Code</label><input readonly="readonly" data-bind="value: Code" />
                        </div>
                        <div class="col-xs-6">
                            <label>User</label><input readonly="readonly" data-bind="value: User" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-6">
                            <pre data-bind="text: StackTrace">Stack Trace</pre>
                        </div>
                        <div class="col-xs-6 tabs">
                            <div role="tabpanel">
                                <ul class="nav nav-pills" role="tablist">
                                    <li role="presentation" class="active"><a href="#routeData" aria-controls="routeData" role="tab" data-toggle="tab">Route Data</a></li>
                                    <li role="presentation" data-bind="visible: show(RouteDefaults)"><a href="#routeDefaults" aria-controls="routeDefaults" role="tab" data-toggle="tab">Route Defaults</a></li>
                                    <li role="presentation" data-bind="visible: show(RouteConstraints)"><a href="#routeConstraints" aria-controls="routeConstraints" role="tab" data-toggle="tab">Route Constraints</a></li>
                                    <li role="presentation" data-bind="visible: show(DataTokens)"><a href="#dataTokens" aria-controls="dataTokens" role="tab" data-toggle="tab">Data Tokens</a></li>
                                    <li role="presentation" data-bind="visible: show(QueryString)"><a href="#queryString" aria-controls="queryString" role="tab" data-toggle="tab">QueryString</a></li>
                                    <li role="presentation" data-bind="visible: show(Form)"><a href="#form" aria-controls="form" role="tab" data-toggle="tab">Form</a></li>
                                    <li role="presentation" data-bind="visible: show(Cookies)"><a href="#cookies" aria-controls="cookies" role="tab" data-toggle="tab">Cookies</a></li>
                                    <li role="presentation"><a href="#serverVariables" aria-controls="serverVariables" role="tab" data-toggle="tab">Server Variables</a></li>
                                </ul>
                            </div>
                            <div class="tab-content">
                                <div role="tabpanel" class="tab-pane active" id="routeData" data-bind="template: { name: 'dictionary', data: RouteData }"></div>
                                <div role="tabpanel" class="tab-pane" id="routeDefaults" data-bind="template: { name: 'dictionary', data: RouteDefaults }"></div>
                                <div role="tabpanel" class="tab-pane" id="routeConstraints" data-bind="template: { name: 'dictionary', data: RouteConstraints }"></div>
                                <div role="tabpanel" class="tab-pane" id="dataTokens" data-bind="template: { name: 'dictionary', data: DataTokens }"></div>
                                <div role="tabpanel" class="tab-pane" id="queryString" data-bind="template: { name: 'dictionary', data: QueryString }"></div>
                                <div role="tabpanel" class="tab-pane" id="form" data-bind="template: { name: 'dictionary', data: Form }"></div>
                                <div role="tabpanel" class="tab-pane" id="cookies" data-bind="template: { name: 'dictionary', data: Cookies }"></div>
                                <div role="tabpanel" class="tab-pane" id="serverVariables" data-bind="template: { name: 'dictionary', data: ServerVariables }"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" role="button" aria-disabled="false" data-toggle="modal" data-target="#aspnet-html" data-bind="visible: !!Html"><span>ASP.NET HTML Error Page</span></button>
                    <div>
                        @*<button type="button" id="wit-dialog-save74" disabled="" class="ui-button ui-widget ui-state-default ui-corner-all ui-button-disabled ui-state-disabled ui-button-text-only" role="button" aria-disabled="true"><span class="ui-button-text">Save</span></button>
                            <button type="button" id="wit-dialog-save-close75" disabled="" class="ui-button ui-widget ui-state-default ui-corner-all ui-button-disabled ui-state-disabled ui-button-text-only" role="button" aria-disabled="true"><span class="ui-button-text">Save and close</span></button>*@
                        <button type="button" role="button" aria-disabled="false" data-dismiss="modal"><span>Close</span></button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal" id="aspnet-html" tabindex="-1" role="dialog" aria-labelledby="aspnet-html-title" aria-hidden="true" data-bind="with: errorLog">
    <div>
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title" id="aspnet-html-title"><span>ASP.NET HTML Error Message</span></h4>
                </div>
                <div class="modal-body">
                    <iframe scrolling="auto" data-bind="content: Html"></iframe>
                </div>
                <div class="modal-footer">
                    <div>
                        <button type="button" role="button" aria-disabled="false" data-dismiss="modal"><span>Close</span></button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/html" id="dictionary">
    <table class="dictionary">
        <tbody data-bind="template: { name: 'keyValuePair', foreach: Object.keys($data), as: 'key' }"></tbody>
    </table>
</script>
<script type="text/html" id="keyValuePair">
    <tr data-bind="visible: !($parent[key] instanceof Object)">
        <th data-bind="text: key"></th>
        <td data-bind="text: $parent[key]"></td>
    </tr>
</script>